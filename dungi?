loadstring(game:HttpGet("https://pastebin.com/raw/LNSPZ8WP"))()


local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")

local plr = Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
local mouse = plr:GetMouse()

local targetPart = workspace.Plots.Plot5.PlotArea
local inPlotValue = plr:WaitForChild("InOwnedPlot")

local moving = false

UIS.InputBegan:Connect(function(input, processed)
    if processed then return end
    if input.KeyCode == Enum.KeyCode.F and not moving then
        moving = true

        -- 현재 위치 및 카메라 시점 저장
        local originalCFrame = hrp.CFrame
        local cam = workspace.CurrentCamera
        local originalCamCF = cam.CFrame

        -- 카메라 고정
        local camConn
        camConn = RunService.RenderStepped:Connect(function()
            cam.CFrame = originalCamCF
        end)

        -- 플레이어만 목표 위치로 이동
        hrp.CFrame = targetPart.CFrame + Vector3.new(0, 2, 0)

        -- Plot에 들어갈 때까지 대기
        repeat
            RunService.Heartbeat:Wait()
        until inPlotValue.Value == true

        -- 원래 위치로 복귀
        hrp.CFrame = originalCFrame

        -- 카메라 연결 해제
        if camConn then
            camConn:Disconnect()
        end
        cam.CFrame = originalCamCF

       task.wait(0.1)
        mouse1click() -- Exploit 환경 전용

        moving = false
    end
end)
